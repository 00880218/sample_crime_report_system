{
  "name": "Danish-raja/crime-report-system",
  "description": "A PHP-based crime report system.",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "vlucas/phpdotenv": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "php artisan clear-compiled"
    ]
  },
  "config": {
    "optimize-autoloader": true
  }
}
